<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>@ViewData["Title"] - RConfig Web UI </title>
    <!-- CSS files -->
    <link href="~/css/tabler.min.css?1668287865" rel="stylesheet" />
    <link href="~/css/tabler-flags.min.css?1668287865" rel="stylesheet" />
    <link href="~/css/tabler-payments.min.css?1668287865" rel="stylesheet" />
    <link href="~/css/tabler-vendors.min.css?1668287865" rel="stylesheet" />
    <link href="~/css/demo.min.css?1668287865" rel="stylesheet" />
    <link href="~/css/sweetalert2.min.css" rel="stylesheet" />
    <link href="~/css/fontawesome.css" rel="stylesheet" />
    <style>
        import url('https://rsms.me/inter/inter.css');

        :root {
            --tblr-font-sans-serif: Inter, -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif;
        }
    </style>
</head>

<body>

    <div class="page">
        <!-- Navbar -->
        <header class="navbar navbar-expand-md navbar-light d-print-none">
            <div class="container-xl">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar-menu"
                        aria-controls="navbar-menu" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <h3 class="navbar-brand navbar-brand-autodark d-none-navbar-horizontal pe-0 pe-md-3">
                   <span class="avatar me-2 bg-white">{RConfig}</span>
                </h3>
                <div class="navbar-nav flex-row order-md-last">
                   
                    <div class="nav-item dropdown">
                        <a href="#" class="nav-link d-flex lh-1 text-reset p-0" data-bs-toggle="dropdown"
                           aria-label="Open user menu">
                            <span class="avatar avatar-sm">AU</span>
                            <div class="d-none d-xl-block ps-2">
                                <div>Admin</div>
                            </div>
                        </a>
                        <div class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                           
                            <a href="/Auth/Logoff" class="dropdown-item">Logout</a>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <div class="navbar-expand-md">
            <div class="collapse navbar-collapse" id="navbar-menu">
                <div class="navbar navbar-light">
                    <div class="container-xl">
                        <ul class="navbar-nav">
                          @*  <li class="nav-item">
                                <a class="nav-link" href="./">
                                    <span class="nav-link-icon d-md-none d-lg-inline-block">
                                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-chart-line" width="44" height="44" viewBox="0 0 24 24" stroke-width="2.5" stroke="#000000" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                            <path d="M4 19l16 0" />
                                            <path d="M4 15l4 -6l4 2l4 -5l4 4" />
                                        </svg>
                                    </span>
                                    <span class="nav-link-title">
                                        Usage Metrics
                                    </span>
                                </a>
                            </li>*@
                            <li class="nav-item">
                                <a class="nav-link" href="./">
                                    <span class="nav-link-icon d-md-none d-lg-inline-block">
                                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-list-details" width="44" height="44" viewBox="0 0 24 24" stroke-width="2.5" stroke="#000000" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                            <path d="M13 5h8" />
                                            <path d="M13 9h5" />
                                            <path d="M13 15h8" />
                                            <path d="M13 19h5" />
                                            <path d="M3 4m0 1a1 1 0 0 1 1 -1h4a1 1 0 0 1 1 1v4a1 1 0 0 1 -1 1h-4a1 1 0 0 1 -1 -1z" />
                                            <path d="M3 14m0 1a1 1 0 0 1 1 -1h4a1 1 0 0 1 1 1v4a1 1 0 0 1 -1 1h-4a1 1 0 0 1 -1 -1z" />
                                        </svg>
                                    </span>
                                    <span class="nav-link-title">
                                        Projects
                                    </span>
                                </a>
                            </li>

                        </ul>
                        
                    </div>
                </div>
            </div>
        </div>
        <div class="page-wrapper">
            <!-- Page body -->
            <div class="page-body">
                @RenderBody()
            </div>
            <footer class="footer footer-transparent d-print-none">
                <div class="container-xl">
                    <div class="row text-center align-items-center flex-row-reverse">
                        <div class="col-lg-auto ms-lg-auto">
                            <ul class="list-inline list-inline-dots mb-0">
                                <li class="list-inline-item">
                                    <a href="https://github.com/tabler/tabler" target="_blank"
                                       class="link-secondary" rel="noopener">Source code</a>
                                </li>
                            </ul>
                        </div>
                        <div class="col-12 col-lg-auto mt-3 mt-lg-0">
                            <ul class="list-inline list-inline-dots mb-0">
                                <li class="list-inline-item">
                                    It is an open source project licensed from MIT.
                                    <a href="https://github.com/alizorlu/RConfig" target="_blank" class="link-secondary">RConfig</a>
                                </li>                               
                            </ul>
                        </div>
                    </div>
                </div>
            </footer>
        </div>

        <script src="~/js/tabler.min.js?1668287865" defer></script>
        <script src="~/js/demo.min.js?1668287865" defer></script>
        <script src="~/js/sweetalert2.all.min.js"></script>
        @await RenderSectionAsync("Scripts", required: false)
    </div>
</body>

</html>